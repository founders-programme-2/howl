{
  "name": "howl",
  "version": "1.0.0",
  "description": "Repository for the Founders Program project \"Howl\". May 2019",
  "main": "server.js",
  "scripts": {
    "start:dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon ./src/server.js ",
    "dev:client": "cd ./client && npm start",
    "start": "node ./src/server.js",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "test": "jest ./tests",
    "coverage": "jest --coverage ./tests",
    "heroku-postbuild": "cd client && npm i && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/founders-programme-2/howl.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/founders-programme-2/howl/issues"
  },
  "homepage": "https://github.com/founders-programme-2/howl#readme",
  "dependencies": {
    "airtable": "^0.6.0",
    "cloudinary": "^1.14.0",
    "datauri": "^2.0.0",
    "env2": "^2.2.2",
    "express": "^4.16.4",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "codecov": "^3.7.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "supertest": "^4.0.2"
  }
}
